<div class="row">
  <h5 class="col-xs-6">Study Brains</h5>
  <div class="col-xs-6">
    <%= link_to 'Add Brain', new_research_cohort_brain_path(cohort_id: @cohort.id),
        class: "pull-right btn btn-primary" %>
  </div>
</div>
<% @cohort.brains.each do |brain| %>
<div>
  <b><%= link_to brain.name, edit_research_brain_path(brain) %></b>
</div>
<% end %>
